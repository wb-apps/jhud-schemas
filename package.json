{
  "name": "@jhud/schemas",
  "version": "2.0.0",
  "description": "Schemas for all JHUD apps and components.",
  "repository": {
    "type": "git",
    "url": "https://github.com/wb-apps/jhud-schemas.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && npm run test && npm run build:babel && npm run build:prepub",
    "build:babel": "babel ./build/js/src --out-dir ./build/js/dist",
    "build:prepub": "cp -rf ./{*.md,package.json} ./build/js/dist",
    "clean": "rimraf ./build/js/dist",
    "pub": "npm run build && cd ./build/js/dist && npm publish --access public",
    "test": "tape -r ./build/js/tests/bootstrap.cjs './build/js/tests/**/*.test.js*' | faucet"
  },
  "type": "module",
  "main": "./index.js",
  "dependencies": {
    "@gdbots/schemas": "3.0.1",
    "@triniti/schemas": "3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@gdbots/pbj": "^3.0.0",
    "@jhud/schemas": "file:./build/js/src/",
    "faucet": "^0.0.1",
    "lodash-es": "^4.17.15",
    "md5": "^2.3.0",
    "rimraf": "^3.0.2",
    "tape": "^5.0.1",
    "utf8": "^3.0.0",
    "uuid": "^8.3.0"
  }
}
