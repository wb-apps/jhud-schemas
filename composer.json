{
  "name": "jhud/schemas",
  "homepage": "https://schemas.jenniferhudsonshow.com/",
  "description": "Schemas for all JHUD apps and components.",
  "type": "pbj-schema-store",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.1",
    "gdbots/schemas": "3.0.1",
    "triniti/schemas": "3.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "gdbots/pbjc": "v1.x-dev",
    "symfony/yaml": "^4.4"
  },
  "autoload": {
    "files": [
      "build/php/src/pbj-schemas.php"
    ],
    "psr-0": {
      "": "build/php/src"
    }
  },
  "autoload-dev": {
    "files": [
      "pbj-schema-stores.php"
    ],
    "psr-0": {
      "": "build/php/tests"
    }
  },
  "scripts": {
    "pbjc": [
      "vendor/bin/pbjc",
      "git add .",
      "@test"
    ],
    "test": [
      "vendor/bin/phpunit",
      "npm test"
    ]
  }
}
